{#
    Two Column Layout
    Parallax Header Layout
#}

{% extends "layouts/base/_base_parallax" %}

{% block parallaxBack %}
  {% block headerBlock %}{% endblock %}
{% endblock %}


{% block content %}

  <div id="page--inner--columns" class="two">
    {# MAIN #}
    <main id="content">
      {% block main %}{% endblock %}
    </main>

    {# ASIDE #}
    <aside>
      {% block aside %}{% endblock %}
    </aside>

    {% block bottomWide %}{% endblock %}
  </div>

{% endblock %}
