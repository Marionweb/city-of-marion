{% set nav = menus.subMenu %}

<ul class="nav--sub">
  {% nav item in nav %}
    {% if item.type == "customMenuItem" %}
      <li><a href="{{item.menuItemUrl}}"{% if item.newWindow %} target="_blank"{% endif %}>{{item.menuItemTitle}}</a></li>
    {% elseif item.type == "entryMenuItem" %}
      {% set entry = item.entry.one() %}
      <li><a href="{{entry.url}}">{% if item.titleOverride %}{{item.titleOverride}}{% else %}{{entry.title}}{% endif %}</a></li>
    {% endif %}
  {% endnav %}
</ul>
