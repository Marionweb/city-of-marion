{
    "name": "city-of-marion",
    "version": "2.1.0",
    "description": "City of Marion Craft CMS site",
    "main": "gulpfile.js",
    "author": "Mike Stecker <mike@stecker.me>",
    "copyright": "mikestecker",
    "license": "UNLICENSED",
    "private": true,
    "paths": {
        "src": {
            "base": "./src/",
            "fonts": "./src/fonts/",
            "json": "./src/json/",
            "js": "./src/js/",
            "img": "./src/img/",
            "sprites": "./src/sprites/",
            "styles": "./src/styles/"
        },
        "dist": {
            "base": "./web/",
            "css": "./web/assets/css/",
            "js": "./web/assets/js/",
            "fonts": "./web/assets/fonts/",
            "img": "./web/assets/img/"
        },
        "build": {
            "base": "./build/",
            "css": "./build/css/",
            "fonts": "./build/fonts/",
            "js": "./build/js/",
            "html": "./build/html/",
            "img": "./build/img/",
            "sprites": "./build/sprites/"
        },
        "favicon": {
            "src": "./src/img/favicon_src.png",
            "dest": "./public/img/site/",
            "path": "/img/site/"
        },
        "templates": "./templates/"
    },
    "domains": {
        "live": "cityofmarion.in.gov",
        "local": "cityofmarion.in.dev",
        "critical": "cityofmarion.in.gov"
    },
    "urls": {
        "live": "https://cityofmarion.in.gov/",
        "local": "https://cityofmarion.in.dev/",
        "critical": "https://cityofmarion.in.dev/"
    },
    "vars": {
        "siteCssName": "site.combined.min.css",
        "siteJsName": "site.combined.js",
        "styleName": "master.styl",
        "cssName": "master.css"
    },
    "globs": {
        "distCss": [
            "./node_modules/normalize.css/normalize.css",
            "./build/css/*.css",
            "./src/css/*.css"
        ],
        "img": [
            "./public/img/"
        ],
        "fonts": [
            "./src/fonts/*.{eot,ttf,woff,woff2}"
        ],
        "critical": [
            {
                "url": "",
                "template": "singles/homepage/index"
            },
            {
                "url": "404",
                "template": "404"
            }
        ],
        "download": [
            {
                "url": "https://www.google-analytics.com/analytics.js",
                "dest": "./web/assets/js/"
            }
        ],
        "distJs": [
            "./node_modules/jquery/dist/jquery.slim.min.js",
            "./node_modules/lazysizes/lazysizes.min.js",
            "./node_modules/lazysizes/plugins/bgset/ls.bgset.min.js",
            "./node_modules/picturefill/dist/picturefill.min.js",
            "./node_modules/responsive-social-buttons/dist/social-buttons.js",
            "./node_modules/@fancyapps/fancybox/dist/jquery.fancybox.min.js",
            "./node_modules/choices.js/assets/scripts/dist/choices.min.js",
            "./node_modules/autosize/dist/autosize.min.js",
            "./src/js/modules/*.js",
            "./build/js/*.js"
        ],
        "babelJs": [
            "./src/js/*.js"
        ],
        "siteIcon": "./web/assets/img/site/favicon.*"
    },
    "dependencies": {
        "@fancyapps/fancybox": "^3.2.5",
        "autosize": "^4.0.0",
        "choices.js": "^3.0.2",
        "fg-loadcss": "^1.3.1",
        "font-awesome-stylus": "^4.7.1",
        "fontfaceobserver": "^2.0.13",
        "jquery": "^3.2.1",
        "lazysizes": "^4.0.1",
        "normalize.css": "^7.0.0",
        "picturefill": "^3.0.2",
        "responsive-social-buttons": "^1.0.7"
    },
    "devDependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "browser-sync": "^2.18.13",
        "chalk": "^2.3.0",
        "critical": "^1.0.0",
        "cross-env": "^5.1.1",
        "css-loader": "^0.28.7",
        "css-mqpacker": "^6.0.1",
        "csso-stylus": "0.0.3",
        "del": "^3.0.0",
        "fancy-log": "^1.3.0",
        "git-rev-sync": "^1.9.1",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.0",
        "gulp-cached": "^1.1.1",
        "gulp-concat": "^2.6.1",
        "gulp-copy": "^1.0.1",
        "gulp-cssnano": "^2.1.2",
        "gulp-debug": "^3.1.0",
        "gulp-download": "^0.0.1",
        "gulp-favicons": "^2.2.7",
        "gulp-filter": "^5.0.1",
        "gulp-header": "^1.8.9",
        "gulp-if": "^2.0.2",
        "gulp-imagemin": "^4.0.0",
        "gulp-load-plugins": "^1.5.0",
        "gulp-newer": "^1.3.0",
        "gulp-plumber": "^1.1.0",
        "gulp-postcss": "^7.0.0",
        "gulp-print": "^2.0.1",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "^0.6.1",
        "gulp-rev": "^8.1.0",
        "gulp-size": "^2.1.0",
        "gulp-sourcemaps": "^2.6.1",
        "gulp-streamify": "^1.0.2",
        "gulp-stylus": "^2.6.0",
        "gulp-svg-sprite": "^1.3.7",
        "gulp-uglify": "^3.0.0",
        "jeet": "^7.2.0",
        "lost": "^8.2.0",
        "moment": "^2.19.2",
        "nib": "^1.1.2",
        "pa11y": "^4.13.1",
        "phantomjs-prebuilt": "^2.1.16",
        "postcss-brand-colors": "^2.0.0",
        "postcss-cssnext": "^3.0.2",
        "poststylus": "^1.0.0",
        "rupture": "^0.7.1",
        "vinyl-source-stream": "^1.1.0"
    },
    "scripts": {
        "start": "gulp",
        "build": "gulp build"
    },
    "engines": {
        "node": ">= 4.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:mikestecker/city-of-marion.git"
    },
    "bugs": {
        "email": "mike@stecker.me"
    }
}
